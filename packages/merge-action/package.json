{
  "name": "merge-action",
  "version": "0.0.7",
  "description": "v2 merge action",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "babel-node ../../bin/compile",
    "deploy": "better-npm-run deploy",
    "prepublish": "better-npm-run deploy:prod",
    "lint": "eslint ."
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile --package=merge-action",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "peerDependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "better-npm-run": "0.0.9",
    "moment": "2.14.1",
    "react": "latest",
    "react-dom": "latest",
    "react-redux": "^4.0.0",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.0.0",
    "redux-form": "^5.3.1",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.3",
    "tocco-ui": "^0.1.0",
    "classnames": "~2.2.4"
  }
}
