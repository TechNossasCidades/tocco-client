{
  "name": "tocco-ui",
  "version": "0.1.1",
  "description": "some ui elements",
  "main": "dist/index.js",
  "scripts": {
    "clean": "../../node_modules/rimraf/bin.js dist",
    "compile": "babel-node ../../bin/compile"
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile --package=tocco-ui",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "peerDependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "flatpickr": "^2.6.3",
    "react-autosize-textarea": "^0.4.3",
    "react-quill": "^1.0.0-rc.2",
    "react-select": "^1.0.0-rc.4"
  }
}
