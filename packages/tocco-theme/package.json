{
  "name": "tocco-theme",
  "version": "0.1.0",
  "description": "styling framework",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "babel-node ../../bin/compile",
    "deploy": "",
    "prepublish": ""
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile --package=tocco-ui",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "peerDependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0"
  }
}
