{
  "name": "tocco-prototype",
  "version": "0.1.50",
  "description": "first react prototype",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "babel-node ../../bin/compile",
    "deploy": "better-npm-run deploy",
    "prepublish": "better-npm-run deploy:prod"
  },
  "betterScripts": {
    "deploy": {
      "command": "npm run clean && npm run compile --package=tocco-prototype",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "peerDependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "better-npm-run": "0.0.13",
    "react": "latest",
    "react-dom": "latest",
    "react-redux": "^5.0.0",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-saga": "^0.13.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "tocco-ui": "^0.1.0"
  }
}
